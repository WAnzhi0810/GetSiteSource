{
  "manifest_version": 2,
  "name": "Favicon Retriever",
  "version": "1.0",
  "description": "Retrieves all favicons from the current website",
  "permissions": ["activeTab", "<all_urls>", "downloads"],
  "browser_action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "web_accessible_resources": ["jszip.min.js"]
}
